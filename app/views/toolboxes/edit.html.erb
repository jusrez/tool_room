<%= form_with url:"/toolboxes/#{@toolbox.id}", method: :patch, local: true do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :tool_capacity %>
<%= f.text_field :tool_capacity %>

<%= f.label :checked_out %>
<%= f.text_field :checked_out %>

<%= f.submit 'Update Toolbox' %>
<% end %>