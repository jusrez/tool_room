<%= form_with url: "/toolboxes/#{@toolbox.id}/tools", method: :post, local: true do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %>

  <%= f.label :working_condition %>:
  <%= f.text_field :working_condition %>

  <%= f.label :quantity %>:
  <%= f.text_field :quantity %>

  <%= f.label :toolbox_id %>:
  <%= f.text_field :toolbox_id %>

  <%= f.submit "Create Tool" %>
<% end %>